{
	"presets":
	[
      "react",
      "es2015",
      "stage-0"
	],

	"plugins":[
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-react-display-name"
	],

	"env":
	{

      "development": {
        "plugins": [
          "add-module-exports",
          "transform-decorators-legacy",
          "transform-react-display-name"
        ]
      },

      "server":
      {
        "presets":
        [
          "react",
          "node6",
          "stage-0"
        ],

        "plugins":
        [
          "transform-runtime",
          "add-module-exports",
          "transform-decorators-legacy",
          "transform-react-display-name"
        ]
      }
	}

}